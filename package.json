{
  "name": "@anime-skip/ui",
  "version": "0.0.5",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "./scripts/build.sh",
    "lint": "eslint --ext .js,.ts,.vue src --max-warnings=0",
    "prettier": "prettier --check \"src/**/*.{js,ts,vue}\"",
    "prettier:write": "yarn prettier --write",
    "prepublishOnly": "yarn build",
    "": "",
    "build:tailwind-scss": "tailwindcss-export-config --config=src/lib/tailwind.preset.js --destination=src/lib/scss/generated-config --format=scss --flat=true",
    "build:types": "tsc --build tsconfig.build.json",
    "build:es": "NODE_ENV=production rollup --config rollup.config.js --format es && yarn build:types",
    "build:js": "NODE_ENV=production rollup --config rollup.config.js && yarn build:types",
    "build:js_css": "NODE_ENV=production SEP_CSS=true rollup --config rollup.config.js && yarn build:types"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.4",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.3.0",
    "husky": "^4.3.6",
    "postcss": "^7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "sass": "^1.30.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tailwindcss-export-config": "^2.3.0",
    "typescript": "^4.1.3",
    "vite": "^1.0.0-rc.13"
  }
}
